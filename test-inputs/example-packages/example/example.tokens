;; phone numbers
;; see http://stackoverflow.com/questions/123559/a-comprehensive-regex-for-phone-number-validation
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

PHONE_7_COMPACT = ([2-9]1[02-9]|[2-9][02-9]1|[2-9][02-9]{2})([0-9]{4}) ;

PHONE_7 = ([2-9]1[02-9]|[2-9][02-9]1|[2-9][02-9]{2})[ ]*([.\-][ ]*)?([0-9]{4}) ;

PHONE_10 = ((\([ ]*([2-9]1[02-9]|[2-9][02-8]1|[2-9][02-8][02-9])[ ]*\)|([2-9]1[02-9]|[2-9][02-8]1|[2-9][02-8][02-9]))[ ]*([.\-][ ]*)?)?([2-9]1[02-9]|[2-9][02-9]1|[2-9][02-9]{2})[ ]*([.\-][ ]*)?([0-9]{4}) ;

PHONE_11 = ((\+?1[ ]*([.\-][ ]*)?)?(\([ ]*([2-9]1[02-9]|[2-9][02-8]1|[2-9][02-8][02-9])[ ]*\)|([2-9]1[02-9]|[2-9][02-8]1|[2-9][02-8][02-9]))[ ]*([.\-][ ]*)?)?([2-9]1[02-9]|[2-9][02-9]1|[2-9][02-9]{2})[ ]*([.\-][ ]*)?([0-9]{4}) ;

PHONE_EXT = ((\+?1[ ]*([.\-][ ]*)?)?(\([ ]*([2-9]1[02-9]|[2-9][02-8]1|[2-9][02-8][02-9])[ ]*\)|([2-9]1[02-9]|[2-9][02-8]1|[2-9][02-8][02-9]))[ ]*([.\-][ ]*)?)?([2-9]1[02-9]|[2-9][02-9]1|[2-9][02-9]{2})[ ]*([.\-][ ]*)?([0-9]{4})([ ]*(\#|x\.?|ext\.?|extension)[ ]*([0-9]+))? ;
