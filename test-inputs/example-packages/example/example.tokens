HEX_LETTER  = [a-f] ;

OCT_DIGIT   = [0-7] ;
DEC_DIGIT   = [0-9] ;
HEX_DIGIT   = [0-9a-f] ;
OCTAL       = 0 [0-7]* ;
DECIMAL     = [1-9] [0-9]* ;
HEXADECIMAL = [0-9a-f]+ ;

FLOAT = \-? (0 | [1-9] [0-9]*) (\. [0-9]+)? ([Ee] [+\-]? (0 | [1-9] [0-9]*))? ;

;; see http://stackoverflow.com/questions/800813/what-is-the-most-difficult-challenging-regular-expression-you-have-ever-written
ROMAN_NUMERAL = M{0,4}(CM|CD|D?C{0,3})(XC|XL|L?X{0,3})(IX|IV|V?I{0,3}) ;

